
@{
    ViewBag.Title = "Goias";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container" ng-app="appMonitor">
    <div class="row">
        <div class="col-md-12" ng-controller="goiasCtrl" ng-init="atualizaCasa()">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Goias</h3>
                </div>
                <div class="panel-body">
                    <ul class="list-group">
                        <li class="list-group-item">Venda Atípica:&nbsp;<input type="checkbox" ng-change="bloquearPlano(2630, 526)" id="planoPagamentoCad" ng-model="planoBloqueado" /></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

